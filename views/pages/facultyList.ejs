<%- include('../components/header'); -%>
<%- include('../components/navbar'); -%>

<section class='section'>
    <div class='section__content section__content--full-width'>
        <div class='jumbotron masthead text-center'>
            <div class='section__title section__title--centered'>
                Faculty list
            </div>
        </div>
    </div>
</section>

<section class="section section__grey">
    <div class="container">
        <div class="row justify-content-md-center">
            <div class="col-md-6 col-sm-12">
                <h3>Search</h3>
                <hr>
                <input class="form-control" id="myInput" type="text"
                       placeholder="e.g. Technology">
            </div>
        </div>
        <br>
        <h3>Full list</h3>
        <hr>
        <ul class="list-group" id="myList">
            <% for (let i = 0; i < profileInfo.length; i++){ %>
                <% if (profileInfo[i].status === "faculty" || profileInfo[i].status === "admin") { %>
                    <li class="list-group-item" style="height: 200px">
                        <div class="row">
                            <div class="col-md-1 col-sm-1 align-self-center">
                                <% if (profileInfo[i].profilePicURL) { %>
                                    <img class="rounded-circle"
                                         src="<%= profileInfo[i].profilePicURL %>"
                                         width="100px" height="100px"
                                         style="object-fit: cover"
                                         alt="profile picture">
                                <% } else { %>
                                    <img class="rounded-circle"
                                         src="data:image/png;base64,"
                                         width="100px" height="100px"
                                         alt="profile picture">
                                <% } %>
                            </div>
                            <div class="col-md-1 col-sm-1 align-self-center">
                                <a href="/profile/view/<%= profileInfo[i]._id %>"><%= profileInfo[i].userName %></a>
                            </div>
                            <div class="col-md-6 col-sm-6 align-self-center">
                                <b>Bio: </b><br> <textarea disabled rows="6"
                                                           style="width: 100%"><%= profileInfo[i].bio %></textarea>
                            </div>
                            <div class="col-md-3 col-sm-3 align-self-center">
                                <b>Affiliation: </b><br><%= profileInfo[i].affiliation %>
                                <br>
                                <b>Department: </b><br><%= profileInfo[i].department %>
                                <br>
                                <b>State: </b><br><%= profileInfo[i].state %>

                            </div>
                            <div class="col-md-1 col-sm-1 align-self-center">
                                <% if (loggedIn) { %>
                                    <% if (!user._id.equals(profileInfo[i]._id)) { %>
                                        <a href="/messages/view/<%= user._id %>/<%= profileInfo[i]._id %>/general"><input
                                                    class="btn btn-sm btn-outline-primary" type="button"
                                                    value="CONTACT"></a>
                                    <% } %>
                                    <% if (user.status === 'admin') { %>
                                        <br><br>
                                        <a href="/profile/update/<%= profileInfo[i]._id %>"><input
                                                    class="btn btn-sm btn-outline-primary" type="button"
                                                    value="Edit"></a>
                                    <% } %>
                                <% } %>
                            </div>
                        </div>
                    </li>
                <% } %>
            <% } %>
        </ul>
    </div>
</section>

<script>
    $(document).ready(function () {
        $("#myInput").on("keyup", function () {
            var value = $("#myInput").val().toLowerCase();
            $("#myList li").filter(function () {
                $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
            });
        });
    });
</script>

<%- include('../components/footer'); -%>
